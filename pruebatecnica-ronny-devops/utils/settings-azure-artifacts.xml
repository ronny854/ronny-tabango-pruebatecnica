<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <profiles>
        <profile>
            <id>zfactorial</id>
            <repositories>
                <repository>
                    <id>feed-artifacts-java</id>
                    <url>https://pkgs.dev.azure.com/zfactorial/Zfactorial_Corp/_packaging/feed-artifacts-java/maven/v1</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>zfactorial</activeProfile>
    </activeProfiles>
    <servers>
        <server>
            <id>feed-artifacts-java</id>
            <username>zfactorial</username>
            <password>_ARTIFACTS_TOKEN_</password>
        </server>
    </servers>
</settings>